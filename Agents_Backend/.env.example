# ============================================
# AGENTS_BACKEND ENVIRONMENT TEMPLATE
# ============================================
# Copy this file to .env and fill with real values
# DO NOT COMMIT .env TO GIT - IT CONTAINS SECRETS

# --- GOOGLE APPS SCRIPT IDs ---
GAS_PORTAIL_ID=your-gas-portail-id
GAS_LIVREUR_ID=your-gas-livreur-id

# --- SENTINEL CONFIGURATION ---
SERVICE_ACCOUNT_KEY=./keys/service-account.json
WORKER_PASS=your-worker-password

# --- VPS CONFIGURATION (OVH) ---
VPS_HOST=xx.xx.xx.xx
VPS_USER=ubuntu
VPS_PASS=your-vps-password

# --- CLOUDFLARE ---
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token

# --- AI & APIs ---
GEMINI_API_KEY=your-gemini-api-key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
TOKEN_TESSIE=your-tessie-token

# --- GOOGLE DRIVE IDs ---
ID_DOSSIER_FACTURES=your-drive-folder-id
ID_JOURNAL_AGENTS=your-drive-sheet-id
ID_FEUILLE_CALCUL=your-sheet-id
ID_LOGO=your-logo-file-id
ID_LOGO_FACTURE=your-logo-facture-id
ID_MODELE_FACTURE=your-template-id
ID_DOCUMENT_CGV=your-cgv-doc-id
ID_CALENDRIER=your-calendar-id
ID_DOSSIER_ARCHIVES=your-archives-folder-id
ID_DOSSIER_TEMPORAIRE=your-temp-folder-id
FOLDER_ID=your-main-folder-id

# --- BUSINESS INFO ---
NOM_ENTREPRISE=Your Company Name
SIRET=00000000000000
IBAN=FR00 0000 0000 0000 0000 0000 000
BIC_ENTREPRISE=XXXX XX XX XXX
ADRESSE_ENTREPRISE=123 Rue Example, 00000 Ville
EMAIL_ENTREPRISE=contact@example.com
ADMIN_EMAIL=admin@example.com

# --- SECRETS ---
ELS_SHARED_SECRET=your-shared-secret
TRACE_SECRET=your-trace-secret
SERVICE_KEY=your-supabase-service-key

# --- TESLA ---
VIN_TESLA_JUNIPER_2025=your-tesla-vin
active_strategy=ECO

# --- LEGACY (LIVREUR APP) ---
CFG_ENABLE_ASSISTANT_OVERRIDE=false
GEMINI_API_KEY_LIVREUR=your-livreur-gemini-key
GOOGLE_MAPS_API_KEY_LIVREUR=your-livreur-maps-key
